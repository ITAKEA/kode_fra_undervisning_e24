    "from data import persons, avg_age\n",
    "from flask import Flask, jsonify\n",
    "\n",
    "app = Flask(__name__)\n",
    "\n",
    "@app.route('/', methods=['GET'])\n",
    "def index():\n",
    "    return jsonify(persons)\n",
    "\n",
    "@app.route('/avg', methods=['GET'])\n",
    "def avg():\n",
    "    return jsonify(avg_age())\n",
    "\n",
    "app.run()"
   ]
  }
